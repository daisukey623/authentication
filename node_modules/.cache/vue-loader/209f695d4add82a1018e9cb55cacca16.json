ult {\n  data() {\n    return {\n      name: '',\n      comment: '',\n      posts: []\n    };\n  },\n  created() {\n    axios.get('/comments').then(response => {\n      this.posts = response.data.documents;\n    });\n  },\n  methods: {\n    createComment() {\n      axios.post('/comments', {\n        fields: {\n          name: {\n            stringValue: this.name\n          },\n          comment: {\n            stringValue: this.comment\n          }\n        }\n      });\n      this.name = '';\n      this.comment = '';\n    }\n  }\n};\n</script>\n\n\n"]}]}